{
  "name": "server",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "seed": "bun run scripts/seed.ts",
    "start": "bun run dist/index.mjs"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.3.0",
    "@sitorstartai/db": "workspace:*",
    "@sitorstartai/env": "workspace:*",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "catalog:",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.3",
    "ws": "^8.19.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@sitorstartai/config": "workspace:*",
    "@types/bcryptjs": "^2.4.6",
    "@types/bun": "catalog:",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "tsdown": "^0.16.5",
    "typescript": "catalog:"
  }
}
